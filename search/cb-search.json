{
	"code" : 0 ,
	"data" : [
	 {% for post in site.posts %}
	{
		"title" : "{{ post.title }} - {% for tag in post.tags %}{% if forloop.rindex != 1 %}{{ tag }}_{% else %}{{ tag }}{% endif %}{% endfor %} - {{ post.date | slice: 0,7 }}",
		"url" : "{{ post.url }}",
		"content" : "{{post.content}}"
	}
	{% if forloop.rindex != 1  %}
	,
	{% endif %}
    {% endfor %}
	]
}
